# -*- mode: ini -*-
# vi: set ft=ini :
[user]
    name = Schlueter
    email = bs@schlueter.blue
[core]
    excludesfile = /Users/blue/.gitignore_global
    pager = less -R
[difftool]
    prompt = false
[push]
    default = current
[color]
    ui = true
[pull]
    rebase = true
[credential]
    helper = osxkeychain
[fetch]
    prune = true
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
